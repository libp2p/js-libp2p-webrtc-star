    "test": "lerna run test",
    "test:node": "lerna run test:node",
    "test:browser": "lerna run test:browser",
    "test:webworker": "lerna run test:webworker",
    "test:electron-main": "lerna run test:electron-main",
    "test:electron-renderer": "lerna run test:electron-renderer",
    "build": "lerna run build",
    "lint": "lerna run lint",
    "release": "lerna run build && lerna publish",
    "dep-check": "lerna run dep-check"
  },
  "homepage": "https://github.com/ipfs/js-ipfs-interfaces/tree/master#readme",
