    "js-mafmt": "^1.0.0",
    "mafmt": "^2.0.2",
    "minimist": "^1.2.0",
    "peer-id": "^0.6.6",
    "peer-info": "^0.6.2",
    "run-parallel": "^1.1.6",
    "simple-peer": "^6.0.4",
    "socket.io": "^1.4.6",
    "socket.io-client": "^1.4.6"
  },
