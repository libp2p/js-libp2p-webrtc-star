  "license": "(Apache-2.0 OR MIT)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/js-ipfs-interfaces.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "lerna": "^4.0.0",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "npm": ">=7.0.0"
  }
}
