    "url": "https://github.com/diasdavid/js-libp2p-webrtc-star/issues"
  },
  "homepage": "https://github.com/diasdavid/js-libp2p-webrtc-star#readme",
  "devDependencies": {
    "aegir": "^3.0.4",
    "bl": "^1.1.2",
    "chai": "^3.5.0",
    "pre-commit": "^1.1.3",
    "webrtcsupport": "^2.2.0"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "duplex-passthrough": "github:diasdavid/duplex-passthrough",
